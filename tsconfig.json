{
    "compilerOptions": {
        "target": "es2020",
        "moduleResolution": "node",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "allowSyntheticDefaultImports": true,
        "module": "es6",
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "declaration": true,
        "incremental": true
    }
}
