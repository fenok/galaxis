        "format": "prettier --write \"**/*.{js,ts,tsx,json,md,*rc}\""
    },
    "devDependencies": {
        "@fetcher/core": "^0.0.19",
        "@typescript-eslint/eslint-plugin": "^4.20.0",
        "@typescript-eslint/parser": "^4.20.0",
        "eslint": "^7.23.0",
        "eslint-config-prettier": "^8.1.0",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "typescript": "^4.2.3"
    },
    "peerDependencies": {
        "@fetcher/core": "^0.0.19"
    },
    "dependencies": {
        "serialize-error": "^8.0.0"
    }
}
