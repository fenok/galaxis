    "keywords": [
        "fetcher",
        "cache",
        "redux-devtools"
    ],
    "version": "0.0.19",
    "repository": "github:fenok/fetcher",
    "license": "MIT",
    "author": "Leonid Fenko <fenok2112@gmail.com>",
    "sideEffects": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "prepare": "yarn build",
        "prepublishOnly": "yarn lint",
        "build": "rimraf ./dist && tsc",
